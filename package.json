{
  "name": "tili",
  "version": "0.10.0",
  "description": "Small javascript utilities.",
  "unpkg": "dist/tili.min.js",
  "main": "dist/tili.js",
  "module": "dist/tili.mjs",
  "author": "Wesley Luyten",
  "license": "ISC",
  "repository": "luwes/tili",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist coverage",
    "test": "nyc ava",
    "lint": "eslint src test",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build:es": "cross-env NODE_ENV=es rollup -c -o dist/tili.mjs",
    "build:umd": "cross-env NODE_ENV=umd rollup -c -o dist/tili.js",
    "build:umd:min": "cross-env NODE_ENV=umd:min rollup -c -o dist/tili.min.js",
    "build": "npm run build:es && npm run build:umd && npm run build:umd:min",
    "watch": "cross-env NODE_ENV=es rollup -w -c -o dist/tili.mjs",
    "prepare": "npm run clean && npm run lint && npm run build",
    "prepublishOnly": "agadoo",
    "docs": "jsdoc2md src/*.js -c jsdoc.config.js > API.md"
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "test/*.js"
    ],
    "sources": [
      "src/*.js"
    ],
    "require": [
      "esm"
    ]
  },
  "nyc": {
    "include": [
      "src/*.js",
      "src/internal/_round.js"
    ],
    "exclude": [
      "src/__.js",
      "src/index.js",
      "src/round.js"
    ],
    "require": [
      "esm"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-export-default-from": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "agadoo": "^1.0.1",
    "ava": "^1.0.0-beta.8",
    "babel-eslint": "^8.2.3",
    "codecov": "^3.0.2",
    "cross-env": "^5.2.0",
    "delay": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-plugin-ava": "^4.5.1",
    "esm": "^3.0.84",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-bundle-size": "^1.0.2",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "uglify-js": "^3.4.0",
    "vm": "^0.1.0"
  }
}
