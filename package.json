{
  "name": "tili",
  "version": "0.8.0",
  "description": "Small javascript utilities.",
  "main": "lib/tili.js",
  "module": "es/tili.js",
  "author": "Wesley Luyten",
  "license": "ISC",
  "repository": "luwes/tili",
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "test": "nyc ava",
    "lint": "eslint src test",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build:cjs": "cross-env NODE_ENV=cjs rollup -c -o lib/tili.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o es/tili.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/tili.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/tili.min.js",
    "build": "npm run build:cjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "watch": "cross-env BABEL_ENV=es NODE_ENV=es rollup -w -c -o es/tili.js",
    "prepare": "npm run clean && npm run lint && npm run build",
    "predocs": "npm run build:cjs",
    "docs": "jsdoc2md lib/*.js > API.md"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "sources": [
      "src/*.js"
    ],
    "require": [
      "esm",
      "@babel/register"
    ]
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "esm",
      "@babel/register"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta",
    "@babel/plugin-syntax-export-default-from": "^7.0.0-beta",
    "@babel/plugin-transform-object-assign": "^7.0.0-beta",
    "@babel/plugin-transform-runtime": "^7.0.0-beta",
    "@babel/preset-env": "^7.0.0-beta",
    "@babel/register": "^7.0.0-beta",
    "@babel/runtime": "^7.0.0-beta",
    "ava": "^1.0.0-beta.6",
    "babel-eslint": "^8.2.3",
    "codecov": "^3.0.2",
    "cross-env": "^5.2.0",
    "delay": "^3.0.0",
    "eslint": "^4.19.1",
    "esm": "^3.0.51",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.7",
    "rollup-plugin-babel": "^4.0.0-beta.5",
    "rollup-plugin-bundle-size": "vimeo/rollup-plugin-bundle-size",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "uglify-js": "^3.4.0",
    "vm": "^0.1.0"
  }
}
