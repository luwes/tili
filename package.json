{
  "name": "tili",
  "version": "0.0.1",
  "description": "Small javascript utilities.",
  "main": "lib/tili.js",
  "module": "es/tili.js",
  "author": "Wesley Luyten",
  "license": "ISC",
  "repository": "luwes/tili",
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write '{src,test}/**/*.js'",
    "format:check": "prettier --list-different '{src,test}/**/*.js'",
    "lint": "eslint src test",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/tili.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o es/tili.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/tili.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/tili.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "prepare": "npm run clean && npm run format:check && npm run lint && npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta",
    "@babel/preset-env": "^7.0.0-beta",
    "babel-eslint": "^7.2.3",
    "chalk": "^2.1.0",
    "chokidar": "^1.7.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.13.1",
    "maxmin": "^2.1.0",
    "prettier": "1.9.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.2",
    "rollup-plugin-babel": "^4.0.0-beta",
    "rollup-plugin-bundle-size": "https://github.com/vimeo/rollup-plugin-bundle-size",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-js": "^3.2.2"
  }
}
